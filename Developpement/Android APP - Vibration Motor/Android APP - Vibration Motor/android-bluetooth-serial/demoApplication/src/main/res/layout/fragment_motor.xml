<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/front_danger_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="5dp"
                    android:fontFamily="@font/open_sans"
                    android:text="@string/front_danger"
                    android:textAlignment="textEnd"
                    android:textAllCaps="true"
                    android:textSize="6pt"
                    android:textStyle="bold" />

            </LinearLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"

                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/motor_1_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="@font/open_sans"
                        android:text="@string/frequency"
                        android:textAlignment="center"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/btn_motor_1" />

                    <TextView
                        android:id="@+id/motor_1_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="@font/open_sans"
                        android:paddingStart="10dp"
                        android:text="@string/undefined_motor"
                        android:textAlignment="viewStart"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_1"
                        app:layout_constraintStart_toEndOf="@+id/motor_1_label"
                        app:layout_constraintTop_toTopOf="@+id/btn_motor_1" />

                    <Button
                        android:id="@+id/btn_motor_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/set_motor"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="DuplicateSpeakableTextCheck" />

                    <TextView
                        android:id="@+id/motor_2_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="@font/open_sans"
                        android:text="@string/pulse"
                        android:textAlignment="center"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_2"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/btn_motor_1" />

                    <TextView
                        android:id="@+id/motor_2_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="@font/open_sans"
                        android:paddingStart="10dp"
                        android:text="@string/undefined_motor"
                        android:textAlignment="viewStart"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_2"
                        app:layout_constraintStart_toEndOf="@+id/motor_2_label"
                        app:layout_constraintTop_toBottomOf="@+id/btn_motor_1" />

                    <Button
                        android:id="@+id/btn_motor_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/set_motor"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/btn_motor_1" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>


            <LinearLayout
                android:id="@+id/linearLayout6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/right_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="5dp"
                    android:fontFamily="@font/open_sans"
                    android:text="@string/lateral_danger"
                    android:textAlignment="textEnd"
                    android:textAllCaps="true"
                    android:textSize="6pt"
                    android:textStyle="bold" />

            </LinearLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"

                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/motor_5_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:fontFamily="@font/open_sans"
                        android:text="@string/frequency"
                        android:textAlignment="center"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/btn_motor_5" />

                    <TextView
                        android:id="@+id/motor_5_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:fontFamily="@font/open_sans"
                        android:paddingStart="10dp"
                        android:text="@string/undefined_motor"
                        android:textAlignment="viewStart"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_5"
                        app:layout_constraintStart_toEndOf="@+id/motor_5_label"
                        app:layout_constraintTop_toTopOf="@+id/btn_motor_5" />

                    <Button
                        android:id="@+id/btn_motor_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/set_motor"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/motor_6_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:fontFamily="@font/open_sans"
                        android:text="@string/pulse"
                        android:textAlignment="center"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_6"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/btn_motor_5" />

                    <TextView
                        android:id="@+id/motor_6_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:fontFamily="@font/open_sans"
                        android:paddingStart="10dp"
                        android:text="@string/undefined_motor"
                        android:textAlignment="viewStart"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_6"
                        app:layout_constraintStart_toEndOf="@+id/motor_6_label"
                        app:layout_constraintTop_toBottomOf="@+id/btn_motor_5" />

                    <Button
                        android:id="@+id/btn_motor_6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/set_motor"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/btn_motor_5" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>


            <LinearLayout
                android:id="@+id/linearLayout8"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/right_near_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/open_sans"
                    android:text="@string/lateral_near"
                    android:textAlignment="textEnd"
                    android:textAllCaps="true"
                    android:textSize="6pt"
                    android:textStyle="bold" />

            </LinearLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"

                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/motor_7_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="7"
                        android:fontFamily="@font/open_sans"
                        android:text="@string/frequency"
                        android:textAlignment="center"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_7"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/btn_motor_7" />

                    <TextView
                        android:id="@+id/motor_7_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="7"
                        android:fontFamily="@font/open_sans"
                        android:paddingStart="10dp"
                        android:text="@string/undefined_motor"
                        android:textAlignment="viewStart"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_7"
                        app:layout_constraintStart_toEndOf="@+id/motor_7_label"
                        app:layout_constraintTop_toTopOf="@+id/btn_motor_7" />

                    <Button
                        android:id="@+id/btn_motor_7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/set_motor"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/motor_8_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="7"
                        android:fontFamily="@font/open_sans"
                        android:text="@string/pulse"
                        android:textAlignment="center"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_8"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/btn_motor_7" />

                    <TextView
                        android:id="@+id/motor_8_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="7"
                        android:fontFamily="@font/open_sans"
                        android:paddingStart="10dp"
                        android:text="@string/undefined_motor"
                        android:textAlignment="viewStart"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_8"
                        app:layout_constraintStart_toEndOf="@+id/motor_8_label"
                        app:layout_constraintTop_toBottomOf="@+id/btn_motor_7" />

                    <Button
                        android:id="@+id/btn_motor_8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/set_motor"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/btn_motor_7" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <LinearLayout
                android:id="@+id/linearLayout10"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/left_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="9dp"
                    android:fontFamily="@font/open_sans"
                    android:text="@string/lateral_far"
                    android:textAlignment="textEnd"
                    android:textAllCaps="true"
                    android:textSize="6pt"
                    android:textStyle="bold" />

            </LinearLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"

                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/motor_9_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="9"
                        android:fontFamily="@font/open_sans"
                        android:text="@string/frequency"
                        android:textAlignment="center"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_9"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/btn_motor_9" />

                    <TextView
                        android:id="@+id/motor_9_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="9"
                        android:fontFamily="@font/open_sans"
                        android:paddingStart="10dp"
                        android:text="@string/undefined_motor"
                        android:textAlignment="viewStart"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_9"
                        app:layout_constraintStart_toEndOf="@+id/motor_9_label"
                        app:layout_constraintTop_toTopOf="@+id/btn_motor_9" />

                    <Button
                        android:id="@+id/btn_motor_9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/set_motor"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/motor_10_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="9"
                        android:fontFamily="@font/open_sans"
                        android:text="@string/pulse"
                        android:textAlignment="center"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_10"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/btn_motor_9" />

                    <TextView
                        android:id="@+id/motor_10_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="9"
                        android:fontFamily="@font/open_sans"
                        android:paddingStart="10dp"
                        android:text="@string/undefined_motor"
                        android:textAlignment="viewStart"
                        app:layout_constraintBottom_toBottomOf="@+id/btn_motor_10"
                        app:layout_constraintStart_toEndOf="@+id/motor_10_label"
                        app:layout_constraintTop_toBottomOf="@+id/btn_motor_9" />

                    <Button
                        android:id="@+id/btn_motor_10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/set_motor"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/btn_motor_9" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>


        </LinearLayout>


    </androidx.core.widget.NestedScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>